<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team2.dao.FileMapper">
	<resultMap type="com.team2.database.FileInfoObject" id="FileResultMap">
        <result property="id" column="ID"/>
        <result property="c_type" column="C_TYPE"/>
        <result property="length" column="LENGTH"/>
        <result property="filename" column="FILENAME"/>

    </resultMap>
 
    <!-- id의 경우에는 Mapper.java에 동일하게 사용해야된다. -->
    <select id="insertfile" resultMap="FileResultMap">
        SELECT * FROM FILE_INFO
    </select>
    
</mapper>